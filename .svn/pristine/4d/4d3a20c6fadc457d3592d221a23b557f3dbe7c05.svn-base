}
.line{
    content: '';
    bottom: 14px;
    right: -65px;
    width: 50px;
    height: 2px;
    background-color: #00aeef;
    text-align: center;
    float: right;
}
.align{
    text-align: center;
}
.fa-arrow-right{
    font-size: 9px;
}
