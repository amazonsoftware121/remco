<div>
    <h3>Company Detailes</h3>
    <div class="col-md-12">
      <div class="row pb-3 center">
        <span class="col-4 tags"><b> </b></span>
        <!-- <img *ngIf="value.profile" [src]="base_url +'uploads/Profile/'+ value.profile" alt="image" class="imagee"> -->
        <img src="./assets/images/download.jpg" alt="image">
        <!-- <img *ngIf="value.profile" [src]="base_url +'uploads/Profile/'+ value.profile" alt="image" class="imagee"> -->
      </div>
      <div class="row pb-3 pt-4 center">
        <span class="col-4 tags"><b>Company Name</b></span>
        <span class="pull-right">:</span>
        <span class="col-7 value">{{value.company_name}}</span>
      </div>

      <div class="row pb-3 center">
        <span class="col-4 tags"><b>Email</b></span>
        <span class="pull-right">:</span>
        <span class="col-7 value">{{value.company_email}}</span>

      </div>
      <div class="row pb-3 center">
        <span class="col-4 tags"><b>Web Site URL</b></span>
        <span class="pull-right">:</span>
        <span class="col-7 value">{{value.company_website}}</span>

      </div>
      <div class="row pb-3 center">
        <span class="col-4 tags"><b>Description</b></span>
        <span class="pull-right">:</span>
        <span class="col-7 value">{{value.company_description}}</span>

      </div>
      <button class="btn btn-primary mr-3 size" (click)="close()">Close</button>
    </div>
  </div>