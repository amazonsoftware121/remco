.form-row{
    justify-content: center;
}
.contents{
    width: 100%;
    text-align: center;
    padding-top: 100px;
    padding-bottom: 50px;
}
.title1{
    font-size: 35px;
    padding: 20px;
}
.contentss{
    font-size: 25px;
}
.mr-3, .mx-3 {
    margin-right: 0px !important;
    margin-top: 10px;
    border-radius: 10px;
}
.form-row > .col, .form-row > [class*=col-] {
    padding-left: 40px;
    padding-right: 40px;
}
.mat-dialog-container {
    display: block;
    padding: 24px;
    border-radius: 4px;
    box-sizing: border-box;
    overflow: auto;
    outline: 0;
    width: 100%;
    height: 100%;
    min-height: inherit;
    max-height: inherit;
}