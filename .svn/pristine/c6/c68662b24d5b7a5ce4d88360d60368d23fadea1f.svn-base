            <button data-filter=".desigen">desigen</button>
            <button data-filter=".marketing">marketing</button>
            <button data-filter=".analysis">analysis</button>
            <button data-filter=".research">research</button>
          </div>
          <div class="grid">
            <div class="grid-item marketing analysis">
              <div class="posrt-thumb">
                <img src="./assets/images/gallery/1.jpg" alt="portfolio-image">
                <div class="overlay-content">
                  <h3 class="caption">img title</h3>
                  <a href="./assets/images/gallery/1.jpg" data-rel="lightcase:myCollection:slideshow"
