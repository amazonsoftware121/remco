                }
                
                .container>p {
                    font-size: 32px
                }
                
                .question {
                    width: 100%
                }
                .p{
                    margin-left: 35px;
                    color: #fff;
                    font-weight: 500;
                }
                
                .options {
                    position: relative;
                    padding-left: 50px
                }
                
                #options label {
                    display: block;
                    margin-bottom: 15px;
                    font-size: 14px;
                    cursor: pointer
                }
                
                .options input {
                    opacity: 0
                }
                
                .checkmark {
                    position: absolute;
                    top: -1px;
                    left: 0;
                    height: 25px;
                    width: 25px;
                    background-color: rgba(0,0,0,0.5);
                    border: 1px solid #ddd;
                    border-radius: 50%;
                    cursor: not-allowed;
                }
                
                .options input:checked~.checkmark:after {
                    display: block
                }
                
                .options .checkmark:after {
                    content: "";
                    width: 10px;
                    height: 10px;
                    display: block;
                    background: white;
                    position: absolute;
                    top: 50%;
                    left: 50%;
                    border-radius: 50%;
                    transform: translate(-50%, -50%) scale(0);
                    transition: 300ms ease-in-out 0s
                }
                
                .options input[type="radio"]:checked~.checkmark {
                    background: #16c1f3;
                    transition: 300ms ease-in-out 0s
                }
                
                .options input[type="radio"]:checked~.checkmark:after {
                    transform: translate(-50%, -50%) scale(1)
                }
                
                .btn-primary {
                    background-color: rgba(0,0,0,0.5);

                    color: #ddd;
                    border: 1px solid #ddd
                }
                
                .btn-primary:hover {
                    background-color: #21bf73;
                    border: 1px solid #21bf73
                }
                .pb-2, .py-2 {
                    padding-bottom: 1.5rem !important;
                }
                .btn-success {
                    padding: 5px 25px;
                    background-color: #21bf73
                }
