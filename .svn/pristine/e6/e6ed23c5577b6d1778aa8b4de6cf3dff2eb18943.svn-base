.animated-digit {
    display: inline-flex;
    height: 50px;
    width: 50px;
    color: #fff;
    font-weight: bold;
    background-color: #09465d;
    text-align: center;
    padding: 24px;
    border-radius: 60px;
    border: 2px solid #fff;
    justify-content: center;
    align-items: center;
    font-size: 12pt;
    margin-top: 15px;
    margin-bottom: 15px;
    float: left;
  }